<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs') %>

<body>
    <%- include('./partials/navbar.ejs') %>
    <div class="container">
        <div class="content">
            <p><%= users.length %></p>
            <% if (users.length >0) { %> <% users.forEach(user => { %>
                <h3 class="title">
                    <%= user.firstName %> <%= user.lastName %>
                </h3> 
                <p><%= user.email %></p>

                <% }) %> <% } else { %>
                    <p> Não há usuários para exibir </p>
                <% } %>
        </div>
    </div>
    
    
    <!--
    <% const message = 'Hello World!' %>
    <h1><%= message %></h1> 
    -->
</body>
</html>